    <script type="text/javascript">
        //Mail POST not reflesh
        /*$("#btnGondert").click(*/
        function send() {
            document.getElementById("FormSend").style.display = "none";
            document.getElementById("FormSending").style.display = "block";
                           //burada css active
                             $("#btnGondert").attr("disabled", "true");

                             var theContact = {
                                 name: nameF.value,
                                 mailadress: mailF.value,
                                 sub: subF.value,
                                 message: messageF.value
                             };

                            var theData = JSON.stringify(theContact);
                            $.ajax({
                                type: "POST",
                                url: "/Shared/Postmail",  @* Controller Name / Action name    *@
                                contentType: "application/json; charset=utf-8",

                                data: theData,
                                dataType: "json",
                                success: function (data) {
                                    $("#btnGondert").removeAttr("disabled");
                                    //css active

                                    if (data == "1") { sendComplate(1); }
                                    else sendComplate(0);



                                },
                                error: function (data) {

                                    $("#btnGondert").removeAttr("disabled");
                                    sendComplate(0);
                                           //css active
                                }


                            });

        }/*)*/;
        function sendComplate(fnishCE) {
            if (fnishCE == 1) {

                document.getElementById("FormSendComplateTextId").innerHTML = "Gönderildi";
                document.getElementById("FormSendComplateIconId").className ="FormSendComplateIcon";
            }
            else {

                document.getElementById("FormSendComplateTextId").innerHTML = "HATA <br/>BAĞLANTI KURULAMADI";
                document.getElementById("FormSendComplateIconId").className = "FormSendComplateIconActive";
            }
            document.getElementById("FormSending").style.display = "none";
            document.getElementById("FormComplate").style.display = "block";
            setTimeout(function () {
                document.getElementById("FormSend").style.display = "block";
                document.getElementById("FormComplate").style.display = "none";

            }, 2000);}
    </script>


        function formdisplay() {

            var over = document.getElementById("formchangeoverlay").style.display;

            var panel = document.getElementById("formchangepanel").style.display;

            if (over != "none") {
                document.getElementById("formchangeoverlay").className = "b24-window-overlay";
                document.getElementById("formchangepanel").className = "b24-window-panel b24-window-panel-pos-right";

                document.getElementById("ViewDataormchangeoverlay").style.display = "none"; document.getElementById("formchangepanel").style.display = "none";
            }
            else {
                panel = "";
                document.getElementById("formchangeoverlay").style.display = "";
                document.getElementById("formchangepanel").style.display = "";
                document.getElementById("formchangeoverlay").className = "b24-window-overlay b24-a-fade enter-active b24-a-fade-enter-to";
                document.getElementById("formchangepanel").className = "b24-window-panel b24-window-panel-pos-right b24-a-slide-bottom-enter-active b24-a-slide-bottom-enter-to";
                setTimeout(function () {
                    document.getElementById("formchangeoverlay").className = "b24-window-overlay";
                    document.getElementById("formchangepanel").className = "b24-window-panel b24-window-panel-pos-right";

                })
            }


        }



